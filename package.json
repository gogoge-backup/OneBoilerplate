{
  "name": "oneboilerplate",
  "version": "0.0.1",
  "description": "one of the frontend boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/gogogeNinja/OneBoilerplate.git"
  },
  "homepage": "https://github.com/gogogeNinja/OneBoilerplate",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register ./test/index.test.js",
    "test-watch": "NODE_ENV=test mocha --compilers js:babel-core/register ./test/index.test.js -w",
    "test-cover": "NODE_ENV=test ./node_modules/isparta/bin/isparta cover --report text --report html --include-all-sources ./node_modules/mocha/bin/_mocha -- --require babel-core/register",
    "hmr": "NODE_ENV=development node server.js"
  },
  "keywords": [
    "Frontend",
    "React",
    "Redux"
  ],
  "author": "Webb Lu",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-module-alias": "^1.1.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.5.0",
    "chalk": "^1.1.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "express": "^4.13.4",
    "ignore-styles": "^4.0.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^15.2.1",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  },
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.1.1",
    "redux": "^3.1.2"
  }
}
